<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <!-- <meta name="viewport" content="width=device-width, initial-scale=1.0"> -->
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- <title>Home | E-Shopper</title> -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/prettyPhoto.css" rel="stylesheet">
    <link href="~/Content/price-range.css" rel="stylesheet">
    <link href="~/Content/animate.css" rel="stylesheet">
    <link href="~/Content/main.css" rel="stylesheet">
    <link href="~/Content/responsive.css" rel="stylesheet">

    <!--[if lt IE 9]>
    <script src="~/Scripts/html5shiv.js"></script>
    <script src="~/Scripts/respond.min.js"></script>
    <![endif]-->
    <link rel="shortcut icon" href="~/images/ico/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/images/ico/apple-touch-icon-57-precomposed.png">
</head>
<!--/head-->
<body>
    <header id="header">
        <div class="header-middle" style="background: rgb(52, 48, 48);">
            <!--header-middle-->
            <div class="container">
                <div class="row">
                    <div>
                        <a class="brand pull-left" href="/ShopQuanAo/Index"><span style="font-size: 35px;color: white;">Shop-QuanAo</span></a>
                        <a class="search_box pull-left" style="padding-top: 7px;padding-left: 15px;">
                            <input type="text" placeholder="Search" style="width: 441px;" />
                        </a>
                    </div>
                    <div style="display: -webkit-box;padding-top: 7px;">
                        <div class="shop-menu pull-right">
                            <ul class="nav navbar-nav">
                                <li><a href="/ShopQuanAo/Index" class="active">Trang chủ</a></li>
                                @Html.Action("GiohangPartial", "Giohang")
                                <li style="padding-top: 10px;padding-left: 0px;">@Html.Action("DangNhap", "NguoiDung")</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div><!--/header-middle-->
        <!--/header-bottom-->
    </header><!--/header-->
    <!--slider-->
    <!--/slider-->

    <section>
                <div>
                    @RenderBody()
                </div>
    </section>

    <script src="~/Scripts/jquery.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.scrollUp.min.js"></script>
    <script src="~/Scripts/price-range.js"></script>
    <script src="~/Scripts/jquery.prettyPhoto.js"></script>
    <script src="~/Scripts/main.js"></script>
</body>
</html>
